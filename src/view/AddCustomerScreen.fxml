<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="control.AddCustomerScreenController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="600.0" prefWidth="400.0" spacing="10.0">
         <children>
            <HBox alignment="CENTER_LEFT" layoutX="35.0" layoutY="35.0" prefHeight="100.0" prefWidth="200.0" spacing="15.0">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Label text="Customer ID" />
                     </children>
                  </VBox>
                  <VBox prefHeight="200.0" prefWidth="150.0">
                     <children>
                        <TextField fx:id="customerIdText" editable="false" opacity="0.5" />
                     </children>
                  </VBox>
               </children>
               <padding>
                  <Insets left="25.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" spacing="15.0">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Label text="Customer Name" />
                     </children>
                  </VBox>
                  <VBox prefHeight="200.0" prefWidth="150.0">
                     <children>
                        <TextField fx:id="customerNameText" />
                     </children>
                  </VBox>
               </children>
               <padding>
                  <Insets left="25.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER_LEFT" layoutX="35.0" layoutY="145.0" prefHeight="100.0" prefWidth="200.0" spacing="15.0">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Label text="Address" />
                     </children>
                  </VBox>
                  <VBox prefHeight="200.0" prefWidth="150.0">
                     <children>
                        <TextField fx:id="customerAddressText" />
                     </children>
                  </VBox>
               </children>
               <padding>
                  <Insets left="25.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER_LEFT" layoutX="35.0" layoutY="365.0" prefHeight="100.0" prefWidth="200.0" spacing="15.0">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Label text="Postal Code" />
                     </children>
                  </VBox>
                  <VBox prefHeight="200.0" prefWidth="150.0">
                     <children>
                        <TextField fx:id="postalCodeText" />
                     </children>
                  </VBox>
               </children>
               <padding>
                  <Insets left="25.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER_LEFT" layoutX="35.0" layoutY="475.0" prefHeight="100.0" prefWidth="200.0" spacing="15.0">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Label text="Phone" />
                     </children>
                  </VBox>
                  <VBox prefHeight="200.0" prefWidth="150.0">
                     <children>
                        <TextField fx:id="phoneText" />
                     </children>
                  </VBox>
               </children>
               <padding>
                  <Insets left="25.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER_LEFT" layoutX="35.0" layoutY="541.0" prefHeight="100.0" prefWidth="200.0" spacing="15.0">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Label text="Country" />
                     </children>
                  </VBox>
                  <VBox prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <ComboBox fx:id="countryComboBox" prefWidth="150.0" />
                     </children>
                  </VBox>
               </children>
               <padding>
                  <Insets left="25.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER_LEFT" layoutX="35.0" layoutY="548.0" prefHeight="100.0" prefWidth="200.0" spacing="15.0">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Label text="Division" />
                     </children>
                  </VBox>
                  <VBox prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <ComboBox fx:id="divisionComboBox" prefWidth="150.0" />
                     </children>
                  </VBox>
               </children>
               <padding>
                  <Insets left="25.0" />
               </padding>
            </HBox>
            <HBox alignment="BOTTOM_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Button fx:id="saveButton" mnemonicParsing="false" onAction="#onActionSaveChanges" text="Save" />
                  <Button fx:id="backButton" mnemonicParsing="false" onAction="#onActionCloseScreen" text="Back" />
               </children>
               <padding>
                  <Insets right="25.0" />
               </padding>
            </HBox>
         </children>
         <padding>
            <Insets bottom="15.0" left="25.0" right="25.0" top="25.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
